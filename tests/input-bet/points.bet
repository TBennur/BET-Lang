struct point (x::int, y::int);

fun addPoint (p::point, q::point)::point {
  let (
    r:=(new point)
  ) { 
    r.x := ((p.x) + (q.x));
    r.y := ((p.y) + (q.y));
    r 
  }
};

let (
  p:=(new point),
  q:=(new point),
  r:=(new point)
) { 
  p.x := (1);
  p.y := (7);
  q.x := (3);
  q.y := (9);
  r.x := (5);
  r.y := (11);
  let (
    s:=(addPoint(p, q)),
    t:=(addPoint(p, r)),
    u:=(addPoint(q, r))
  ) { 
    print (s);
    print (t);
    print (u);
    null point 
  } 
}