if (input <= 0){
    0
}else{
    let (
        x := 0,
        i := 0,
        j := 0,
        k := 0
    ){
        do {
            do {
                do {
                    x:=x + 1;
                    k:=k + 1
                } until (input <= k);
                k:=0;
                j:=j + 1
            } until (input <= j);
            j:=0;
            i:=i + 1;
            x
        } until (input <= i)
    }
}