fun ret_true()::bool{
    print(3);
    true
};

fun ret_ret_true()::()->bool{
    print(2);
    ret_true
};

fun call_no_arg(x::()->()->bool)::bool{
    print(1);
    (x())()
};

fun ret_unit()::unit{
    print(3);
    # empty block
};

fun ret_ret_unit()::()->unit {
    print(2);
    ret_unit
};

fun ret_ret_ret_unit()::()->()->unit{
    print(1);
    ret_ret_unit
};


# (int, int)->int
fun plus_int(x::int, y::int)::int {
    print(x);
    print(y);
    print(x + y)
};


# (int, int, ((int, int) -> int)) -> int
fun many_ints(a::int, b::int, c::(int, int)->int)::int {
    print(a);
    print(b);
    c(a, b)
};

many_ints(1, 2, plus_int)

