(fun square ((n int)) int (* n n))

(fun printUntil ((n int)) int
    (
        let ((i 1))
        (block
            (repeat-until 
                (block 
                    (print i) 
                    (print (square i)) 
                    (set! i (add1 i))
                ) 
                (= i n)
            )
            (print i)
            (square i)
        )
    )
)

(printUntil input)